# Example variable values for the development environment

# SVM configuration
svm_name = "dev-svm"
svm_ip = "192.168.1.10"
svm_username = "admin"
svm_password = "your_password"

# Network configuration
network_interface_name = "eth0"
vlan_id = 100
broadcast_domain = "192.168.1.0/24"

# Volume configuration
volume_name = "dev-volume"
volume_size = "100Gi"
volume_type = "flexvol"

# NFS configuration
nfs_share_name = "dev-nfs-share"
nfs_export_path = "/vol/dev-volume"

# CIFS configuration
cifs_share_name = "dev-cifs-share"
cifs_share_path = "/dev-cifs-share"